package com.objectss;

public class Car {
	
	int noOfWheels;
	String color;
	float maxSpeed;
	float currentFuelInLiters;
	int noOfSeats;
	
	public void drive() {
		if(currentFuelInLiters == 0) {
			System.out.println("Car is out of fuel");
		}
		System.out.println("Car is draving..");
		currentFuelInLiters--;
	}
	
	public void addFuel(float fuel) {
	   currentFuelInLiters += fuel;
	}

}
