package com.objectss;

public class Driver {
	
	public static void main(String[] args) {
		
		Car myCar = new Car();
		myCar.addFuel(6);
		myCar.drive();
		System.out.println(myCar.color = "red");
		System.out.println(myCar.currentFuelInLiters);
		
		System.out.println("---------------");
		
		Car myCar1 = new Car();
		myCar.addFuel(6);
		myCar.drive();
		System.out.println(myCar.color = "yellow");
		System.out.println(myCar.currentFuelInLiters);

	}

}
