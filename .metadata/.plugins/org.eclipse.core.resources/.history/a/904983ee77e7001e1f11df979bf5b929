package com.objectss;

public class Driver {
	
	public static void main(String[] args) {
		
		Car myCar = new Car();
		myCar.addFuel(6);
		myCar.drive();
		myCar.color = "res";
		System.out.println(myCar.currentFuelInLiters);
		System.out.println("---------------");
		Car myCar1 = new Car();
		myCar.addFuel(6);
		myCar.drive();
		myCar.color = "yellow";
		System.out.println(myCar.currentFuelInLiters);

	}

}
