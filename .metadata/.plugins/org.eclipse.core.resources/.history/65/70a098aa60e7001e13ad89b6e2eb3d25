package com.challenge;

public class FindOccurrences {
	
	public static int findOccurrences(int[] arr, int num) {
		int count = 0;
		for(int i=0; i<arr.length; i++) {
			if(arr[i] == num) {
				count++;
			}
		}
		return count;
	}

	public static void main(String[] args) {
		int[] intArray = {1, 2, 3, 4, 5,1,2,1,4,3,1,6,4,1};
	}
}
