package com.challenge;

public class MaxAndMin {
	
	public static int maxAndMinGivenArray(int[] arr) {
		int max = Integer.MIN_VALUE;
		int i=0; 
		
		while(i < arr.length) {
			if(arr[i] > max) {
				max = arr[i];
			}
			i++;
		}
		return max;
	}
	
	public static void main(String[] args) {
		
	}

}
